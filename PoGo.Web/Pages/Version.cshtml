@page
@model VersionModel

<div class="form-group row">
    <label for="ApplicationStart" class="col-sm-2 col-form-label">Application Start</label>
    <div class="col-sm-10">
        <input type="text" readonly class="form-control-plaintext" id="ApplicationStart" value="@Model.ApplicationStart">
    </div>
</div>
<div class="form-group row">
    <label for="CurrentDate" class="col-sm-2 col-form-label">Current Time</label>
    <div class="col-sm-10">
        <input type="text" readonly class="form-control-plaintext" id="CurrentDate" value="@Model.CurrentDate">
    </div>
</div>
<div class="form-group row">
    <label for="TotalTime" class="col-sm-2 col-form-label">Total Time</label>
    <div class="col-sm-10">
        <input type="text" readonly class="form-control-plaintext" id="TotalTime" value="@Model.TotalTime">
    </div>
</div>